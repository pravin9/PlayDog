<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.courseworkVersion">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.courseworkVersion.NewGameActivity"></activity>
        <activity android:name="com.example.courseworkVersion.IdentifyBreedTimer"></activity>
        <activity android:name="com.example.courseworkVersion.DogIndentifyTimer"></activity>
        <activity
            android:name="com.example.courseworkVersion.SearchDogBreeds"
            android:parentActivityName="com.example.courseworkVersion.MainActivity" />
        <activity
            android:name="com.example.courseworkVersion.IdentifyBreed"
            android:parentActivityName="com.example.courseworkVersion.MainActivity" />
        <activity
            android:name="com.example.courseworkVersion.DogIndentify"
            android:parentActivityName="com.example.courseworkVersion.MainActivity" />
        <activity
            android:name="com.example.courseworkVersion.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>